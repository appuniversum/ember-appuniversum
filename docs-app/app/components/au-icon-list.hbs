<div class="au-o-grid au-o-grid--small" ...attributes>
  {{#each this.icons as |icon|}}
    <div
      class="au-o-grid__item au-u-1-3@small au-u-1-5@medium au-u-flex au-u-flex--vertical-center"
    >
      <AuIcon
        @icon={{icon.name}}
        @size="large"
        @alignment="left"
        @ariaHidden={{true}}
      />
      <p>{{icon.name}}</p>
    </div>
  {{/each}}
</div>